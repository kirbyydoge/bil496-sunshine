<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/examsobs/db" VERSION="20220305" COMMENT="XMLDB file for Moodle blocks/examsobs"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_examsobs" COMMENT="new values add">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
        <FIELD NAME="examid" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="examID"/>
        <FIELD NAME="email" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="email"/>
        <FIELD NAME="reg_date" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="reg_date"/>
        <FIELD NAME="state" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="state"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
