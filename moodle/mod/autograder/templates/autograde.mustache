<h1 class="m-mod-autograder-title">{{body_title}}</h1>

<div style="margin: 20px auto auto auto; width: 65%;">
    {{#out_buffer}}
        <div class="card rounded calendar_event_course">
            <div class="box card-header clearfix">
                <div class="d-inline-block">
                    <h3 class="name d-inline-block">{{username}}</h3>
                </div>
            </div>
        </div>
        <div class="description card-body">
            <div class="row">
                <div class="col-11">
                    {{#result}}
                        <a>{{.}}</a> <br>
                    {{/result}}
                </div>
            </div>
        </div>
    {{/out_buffer}}
</div>

